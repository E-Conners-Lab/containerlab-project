# E-University MPLS Lab - Environment Variables
# Copy this file to .env and fill in your actual values
# NEVER commit .env to version control

# NetBox Configuration
NETBOX_URL=http://your-netbox-host:8000
NETBOX_TOKEN=your-netbox-api-token-here

# Router Credentials
ROUTER_USERNAME=admin
ROUTER_PASSWORD=your-router-password

# ContainerLab Host SSH
CLAB_HOST=192.168.68.53
CLAB_SSH_USER=your-ssh-username
CLAB_SSH_PASSWORD=your-ssh-password

# NetBox Web UI (for documentation only, not used by scripts)
NETBOX_ADMIN_USER=admin
NETBOX_ADMIN_PASSWORD=your-netbox-admin-password
